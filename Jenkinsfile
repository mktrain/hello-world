pipeline {
    
    agent any
    stages {
         stage("Checkout") {
             steps {
                 echo " Checkingout the code"
             }
         }
         stage("Build") {
             steps {
                 echo " Building the code"
             }
         }
         stage("Test") {
             steps {
                 echo " Testing the code"
             }
         }
         stage("Deploy") {
             steps {
                 echo " Deploying the code"
             }
         }
    }    
}
